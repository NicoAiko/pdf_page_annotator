{
  "env": {
    "browser": true,
    "node": false,
    "es6": true
  },
  "extends": [
    "plugin:vue/vue3-recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "vue/multi-word-component-names": ["off"]
  }
}
